<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>


<VBox alignment="CENTER" spacing="10" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.apptestingjavafx.AboutDialogController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Label style="-fx-font-size: 18;" text="About Program" />
        <Label text="Michal, Ondra, Filip" wrapText="true" />
        <Label text="Version 1.0" />
        <HBox alignment="CENTER_RIGHT" spacing="10">
            <Button mnemonicParsing="false" onAction="#onCloseButtonClick" text="OK" style="-fx-padding: 5px 20px;"  />
        </HBox>
    </children>
</VBox>